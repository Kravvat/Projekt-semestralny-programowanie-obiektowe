<Window x:Class="WyposazenieDlaSilowni.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WyposazenieDlaSilowni"
        mc:Ignorable="d"
        Title="Wyposazenie dla silowni" Height="600" Width="800" FontSize="14" Background="DarkCyan" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <local:Wyposazenie_dla_silowni_DataSet x:Key="wyposazenie_dla_silowni_DataSet"/>
        <CollectionViewSource x:Key="klienciViewSource" Source="{Binding Klienci, Source={StaticResource wyposazenie_dla_silowni_DataSet}}"/>
    </Window.Resources>

    <Grid>
        <Label Content="Strona Glowna"/>
    </Grid>
</Window>
